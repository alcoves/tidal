  
[Unit]
Description=Nomad client
Wants=network-online.target
After=network-online.target
[Service]
ExecStart= /bin/sh -c "cd /home/ubuntu/tidal && git reset --hard origin/dev && git pull && /usr/local/bin/nomad agent -config /home/ubuntu/tidal/nomad/client.hcl"
Restart=always
RestartSec=10
[Install]
WantedBy=multi-user.target