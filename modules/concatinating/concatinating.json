[
  {
    "name": "concatinating",
    "image": "${app_image}",
    "environment": [
      {
        "name": "WASABI_ACCESS_KEY_ID",
        "value": "${wasabi_access_key_id}"
      },
      {
        "name": "WASABI_SECRET_ACCESS_KEY",
        "value": "${wasabi_secret_access_key}"
      }
    ],
    "cpu": 4064,
    "memory": 8128,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
