{
  "libvpx_vp9-2160p": {
    "preset": "libvpx_vp9-2160p",
    "ext": "webm",
    "cmd": [
      "-speed 5",
      "-b:v 0",
      "-crf 40",
      "-c:v libvpx-vp9",
      "-pix_fmt yuv420p",
      "-vf scale=3840:-2"
    ]
  },
  "libx264-2160p": {
    "preset": "libx264-2160p",
    "ext": "mp4",
    "cmd": [
      "-c:v libx264",
      "-preset faster",
      "-profile:v high",
      "-vf scale=3840:-2",
      "-crf 22",
      "-coder 1",
      "-x264opts 'keyint=25:min-keyint=25:no-scenecut'",
      "-vsync passthrough",
      "-pix_fmt yuv420p",
      "-bf 2"
    ]
  },
